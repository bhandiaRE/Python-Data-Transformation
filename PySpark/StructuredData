    {
      "cell_type": "code",
      "source": [
        "df.createOrReplaceTempView(\"Archive\")\n",
        "spark_sqls = \"\"\"\n",
        "            SELECT Protocol,\n",
        "            CASE label\n",
        "              WHEN 'normal.' THEN 'no attack'\n",
        "              ELSE 'attack'\n",
        "            END AS State,\n",
        "            COUNT(*) AS freq\n",
        "            FROM Archive\n",
        "            WHERE guest_login != '1'\n",
        "            GROUP BY Protocol, State\n",
        "            ORDER BY Protocol DESC\n",
        "\"\"\"\n",
        "spark.sql(spark_sqls).show()"
      ],
